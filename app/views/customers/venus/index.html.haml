      .col-sm-12.p-t
        %section.panel.panel-default.m-n
          %header.panel-heading.text-left
            Select Providers and appointments
          %section.panel-body.slim-scroll
            -unless @location_dental.nil?
              %div.provider_sec
                = form_for "appointment_dental" do |f|
                  %h5.font-bold.text.text-primary Dental Provider
                  -if @location_dental.each.count>0
                    - @location_dental.each do |location|
                      %article.media
                        %div.col-sm-6.row
                          .div.pull-left
                            %input.dental_provider{:name => "selectProvider", :type => "radio", :value => "#{location.addressee_id}"}/
                          .media-body.provider_address.col-sm-10
                            %h5.no_margin.dental_provider
                              = location.name
                        .form-group.dental_Appointment.pull-right.col-sm-6.no_margin
                          = f.text_field "#{location.addressee_id}",{class: 'input-sm input-s form-control datetimepicker1 hide dental_input z_idx_id', 'readonly'=> 'readonly', 'data-required' => 'true','placeholder' => 'Select an Appointment'}
                        %div.col-sm-12.addr_hide
                          %small.block.col-sm-12
                            %a.text.text-primary
                              = number_with_precision(location.distance, precision: 2) + " away"
                          %small.block.col-sm-12.dental_provider_address
                            = location.details rescue '-'
                  -else
                    %p There is no dental provider near by your home address
            -unless @location_vision.nil?
              %div.provider_sec
                = form_for "appointment_vision" do |f|
                  %h5.font-bold.text.text-primary Vision Provider
                  -if @location_vision.each.count>0
                    - @location_vision.each do |location|
                      %article.media
                        %div.col-sm-6.row
                          .div.pull-left
                            %input.vision_provider{:name => "visionProvider", :value => "#{location.addressee_id}", :type => "radio"}/
                          .media-body.provider_address.col-sm-11
                            %small.block.pull-right.takeVision_Appointment.hide
                              .form-group.clearfix
                            %h5.no_margin.vision_provider
                              = location.name
                        .form-group.vision_Appointment.pull-right.col-sm-6.no_margin
                          = f.text_field "#{location.addressee_id}",{class: 'input-sm input-s form-control datetimepicker2 hide vision_input z_idx_id', 'readonly'=> 'readonly', 'data-required' => 'true','placeholder' => 'Select an Appointment'}

                        %div.col-sm-12.addr_hide
                          %small.block.col-sm-12
                            %a.text.text-primary
                              = number_with_precision(location.distance, precision: 2) + " away"
                          %small.block.vision_provider_address.col-sm-12
                            = location.details rescue '-'
                  -else
                    %p There is no vision provider near by your home address
            -unless @location_body.nil?
              %div.provider_sec
                = form_for "appointment_body" do |f|
                  %h5.font-bold.text.text-primary Body Checkup
                  -unless @address.nil?
                    %article.media
                      %div.col-sm-6.row
                        .div.pull-left
                          %input{:checked => "checked", :name => "home_provider", :type => "radio", :value => ""}/
                        .media-body.provider_address.col-sm-11
                          %h5.home_provider Your Home
                      .form-group.pull-right.col-sm-6.body_Appointment.no_margin
                        = f.text_field "#{@address.id}",{class: 'input-sm input-s form-control datetimepicker3 body_input z_idx_id', 'readonly'=> 'readonly', 'data-required' => 'true','placeholder' => 'Select an Appointment'}
                      %div.col-sm-12.addr_hide
                        %p.block.col-sm-12
                          = @address.details rescue '-'
            %div.pull-left.p-t.col-sm-12
              = form_tag @url, method: :post, :id => "payment" do |payment|
                %input#key{:name =>"key", :type => "hidden", :value => "#{@key}"}
                %input#txnid{:name =>"txnid", :type => "hidden", :value => "#{@transID}"}
                %input#amount{:name =>"amount", :type => "hidden", :value => "#{@package_info.selling_price}"}
                %input#productinfo{:name =>"productinfo", :type => "hidden", :value => "eKincare"}
                %input#firstname{:name =>"firstname", :type => "hidden", :value => "#{@customer.first_name}"}
                %input#email{:name =>"email", :type => "hidden", :value => "#{@customer.email}"}
                %input{:name =>"phone", :type => "hidden", :value => "91#{@customer.mobile_number}"}
                %input{:name =>"surl", :type => "hidden", :value => "#{request.protocol}#{request.host_with_port}/customers/payment_success"}
                %input{:name =>"furl", :type => "hidden", :value => "#{request.protocol}#{request.host_with_port}/customers/payment_failed"}
                %input#hash{:name =>"hash", :type => "hidden", :value => "#{@hash}"}
                %input#testpro{:name =>"testpro", :type => "hidden", :value => "#{@salt}"}
                %input#service_provider{:name =>"service_provider", :type => "hidden", :value => "payu_paisa"}
                %input#customer_id{:name =>"customer_id", :type => "hidden", :value => "#{@customer_id}"}
                %input#package_id{:name =>"package_id", :type => "hidden", :value => "#{@package_info.id}"}
                %input#city{:name =>"city", :type => "hidden", :value => ""}
                %section.pull-left.col-sm-7#coupon_section
                  %div.col-sm-7.no_padding
                    %input{:id=>"coupon_code",:class=>"form-control",:name =>"coupon", 'placeholder' => 'Apply Coupon', :type => "text", :value => ""}
                  %div.col-sm-2.no_padding
                    %span#coupon_button.btn.btn-success.pull-left.m-r-sm
                      Apply
                %section.pull-left.col-sm-7#coupon_success.hide
                  %div.col-sm-12
                    Coupon Applied Successfully
                %span.pull-right.btn.btn-success.checkOutContinue{:disabled => ""}
                  Proceed to payment
              .slimScrollBar{:style => "width: 10px; position: absolute; top: 0px; opacity: 0.4; display: none; border-radius: 7px; z-index: 99; right: 1px; height: 164.797507788162px; background: rgb(0, 0, 0);"}
              .slimScrollRail{:style => "width: 10px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 7px; opacity: 0.2; z-index: 90; right: 1px; background: rgb(51, 51, 51);"}
