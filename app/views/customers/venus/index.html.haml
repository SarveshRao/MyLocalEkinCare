= content_for :title, 'locations'
%div.clearfix.breadcrumb
  %ol.padder
    = render_breadcrumbs :separator => ' / '
    %small.pull-right
      Contact: 888-678-EKIN (3546) /
      %a{href: 'mailto:founder@ekincare.com'} founder@ekincare.com
%section.vbox
  %section.scrollable.padder.row.package_address{:style => "height:1000px"}
    .col-sm-7
      .col-sm-12
        %section.panel.panel-default.m-n
          %header.panel-heading.text-left.address_heading
            %span.cus Your Current Address
            %a.btn.btn-primary.pull-right#address_info_edit_btn
              %i.fa.fa-pencil
              Update your address
          %section.panel-body.slim-scroll
            %div
              .media-body.provider_address
                %div.col-sm-12
                  %p.block.col-sm-12
                    = @address.details rescue '-'
            -#%div
            -#  %p.block.col-sm-12
            -#  %a.btn.btn-primary.pull-right#address_info_edit_btn
            -#    %i.fa.fa-pencil
            -#    Update your address
