                  %li
                    Comprehensive dental check
                  %li
                    For a family of 4 members
                  %li
                    Free online reports
                -if @package_info.name == "Pro Package"
                  %li
                    60+ test
                  %li
                    Dental & Vision check
                  %li
                    Home collection
                  %li
                    Online Reports
                -if @package_info.name == "Baseline Package"
                  %li
                    30+ test
                  %li
                    Dental & Vision check
                  %li
                    Home collection
                  %li
                    Online Reports
                -if @package_info.name == "Dental Check"
                  %li
                    Comprehensive dental check
                  %li
                    Online Reports
                -if @package_info.name == "Vision Check"
                  %li
                    Comprehensive vision check
                  %li
                    Online Reports
                -if @package_info.name == "Dental & Vision Check"
                  %li
                    Comprehensive vision and dental exam that checks the health of your eyes and teeth
                  %li
                    Online Reports
                -if @package_info.name == "Diabetes Package"
                  %li
                    Tests for average glucose levels over a period of 2-3 months
                  %li
                    Lipid profile
                  %li
                    Home collection
                  %li
                    Online Reports
                -if @package_info.name == "Thyroid Package"
                  %li
                    Includes T3, T4 and TSH tests
                  %li
                    Home collection
                  %li
                    Online Reports
