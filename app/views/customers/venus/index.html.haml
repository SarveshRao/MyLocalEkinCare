                    %strong.h3.dental_time
                    %br
                    %strong.h3.dental_date
                    %strong.h3.dental_month
                    -#%small.label.bg-light.dental_year
                  %p.dental_provider_val
                  %p.dental_provider_address_val
            -unless @location_vision.nil?
              %section.pull-left.col-sm-12.p-t.hide
                %h5.h5.font-bold.text.text-primary
                  Vision Provider
                %div
                  %input.vision_input_val{:name => "vision_input_val", :type => "hidden"}/
                  .pull-right.media-xs.text-center.text-muted
                    %strong.h3.vision_time
                    %br
                    %strong.h3.vision_date
                    %strong.h3.vision_month
                    -#%small.label.bg-light.vision_year
                  %p.vision_provider_val
                  %p.vision_provider_address_val
            -unless @location_body.nil?
              %section.pull-left.col-sm-12.p-t
                %h5.h5.font-bold.text.text-primary
                  Body Checkup
                %div
                %div
                  %input.body_input_val{:name => "body_input_val", :type => "hidden"}/
