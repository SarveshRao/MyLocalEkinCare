              %p#price.eKin_price.font-bold
                = "Rs "+number_with_precision(@package_info.selling_price, precision: 2)
              %p
                %strike.text-danger.text-sm
                  = "Rs "+number_with_precision(@package_info.mrp, precision: 2)
              %p.text-sm
