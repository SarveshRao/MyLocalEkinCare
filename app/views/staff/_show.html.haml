%div.row.col-lg-6
  #basic_details.row.panel.tab-pane.active.m.p-t.m-n
    %div#normal_customer_basic_details
      .col-md-10
        %a.tab-pencil.pull-right#customer_basic_details_edit_btn
          %i.fa.fa-pencil
        .vbox.row
          .col-xs-6
            %p.text-lt Name:
          .col-xs-6
            %p.text-lt
              = @customer.name || '-'
        .vbox.row
          .col-xs-6
            %p.text-lt D.O.B:
          .col-xs-6
            %p.text-lt
              = formatted_date @customer.date_of_birth || '-'
        .vbox.row
          .col-xs-6
            %p.text-lt Age:
          .col-xs-6
            %p.text-lt
              = @customer.age['year'] == '-' ? '-' : "#{@customer.age['year']} years"
        .vbox.row
          .col-xs-6
            %p.text-lt Number of Children:
          .col-xs-6
            %p.text-lt
              = @customer.number_of_children || '-'
        .vbox.row
          .col-xs-6
            %p.text-lt Blood Group:
          .col-xs-6
            %p.text-lt
              = @customer.customer_vitals.blood_group.blood_type rescue '-'

        .vbox.row
          .col-xs-6
            %p.text-lt Height:
          .col-xs-6
            %p.text-lt
              = @customer.height || '-'
        .vbox.row
          .col-xs-6
            %p.text-lt Weight:
          .col-xs-6
            %p.text-lt
              = @customer.customer_vitals.weight || '-'

        .vbox.row
          .col-xs-6
            %p.text-lt Medical Insurance:
          .col-xs-6
            %p.text-lt
              = @customer.medical_insurance || '-'
        .vbox.row
          .col-xs-6
            %p.text-lt Sponsor:
          .col-xs-6
            %p.text-lt
              = @sponsor || '-'

    %div#edit_customer_basic_details_form.hide.row.panel.tab-pane.padder-v.active.m.m-n
      .col-md-6
        = form_for @customer,remote: true, html: {id: 'customer_basic_details_form'}  do |f|
          .form-group
            = f.label 'First Name'
            %sup.text-danger
              *
            = f.text_field :first_name,{class: 'form-control parsley-validated', 'data-required' => 'true', autofocus: true}

          .form-group
            = f.label 'Last Name'
            %sup.text-danger
              *
            = f.text_field :last_name,{class: 'form-control parsley-validated', 'data-required' => 'true'}

          .form-group
            %label D.O.B
            %sup.text-danger
              *
            = f.text_field :date_of_birth, {value: numeric_date_format(@customer.date_of_birth), class: 'input-sm input-s datepicker-input form-control datepicker', 'readonly' => 'readonly', 'data-date-format' => 'dd-mm-yyyy', 'data-required' => 'true', placeholder: 'dd-mm-yyyy'}

          .form-group
            = f.label 'Number of Children'
            = f.text_field :number_of_children, {class: 'form-control', 'data-type' => 'number'}

          .form-group
            .form-group
              = f.label 'Blood Group'
              = select_tag 'blood_group_id', options_for_select(BloodGroup.all.collect{|b| [b.blood_type, b.id]}.insert(0, 'select'), selected: @customer.customer_vitals.blood_group_id),  {class: 'form-control parsley-validated'}

          .form-group
            = f.label :height
            .row
              .col-md-6
                .input-group
                  = f.text_field :feet, {value: @customer.customer_vitals.feet, class: 'form-control'}
                  %span.input-group-btn
                    %button.btn.btn-default.test_component_units{type: "button", class: "unit"}
                      feet

              .col-md-6
                .input-group
                  = f.text_field :inches, {value: @customer.customer_vitals.inches, class: 'form-control'}
                  %span.input-group-btn
                    %button.btn.btn-default.test_component_units{type: "button", class: "unit"}
                      inches

          .form-group
            = f.label :weight
            .input-group
              = f.text_field :weight, {value: @customer.customer_vitals.weight, class: 'form-control'}
              %span.input-group-btn
                %button.btn.btn-default.test_component_units{type: "button", class: "unit"}
                  Kgs


          .form-group
            = f.label 'Medical Insurance'
            = f.select :medical_insurance, options_for_select(['No', 'Yes'], selected: @customer.medical_insurance), {}, {class: 'form-control'}

          .form-group
            = f.label :sponsor
            = f.select :sponsor, options_for_select(Company.all.collect {|p| [ p.name,p.id]}), {:include_blank => "Self", :selected => @sponsor, autofocus: true}, {class: 'form-control m-b parsley-validated'}


          .form-group
            = f.submit 'save', {class:'btn btn-sm small-btn btn-primary'}
            %a{class: 'btn btn-sm small-btn btn-danger customer-details-cancel-btn'} cancel

  #add_details.row.hide.panel.tab-pane.active.m.p-t.m-n
    %div#normal_customer_add_basic_details
      .col-md-10
        %a.tab-pencil.pull-right#customer_add_basic_details_edit_btn
          %i.fa.fa-pencil
        .vbox.row
          .col-xs-6
            %p.text-lt Daily Activity:
          .col-xs-6
            %p.text-lt
              = @customer.daily_activity || '-'
        .vbox.row
          .col-xs-6
            %p.text-lt Frequency of Exercise:
          .col-xs-6
            %p.text-lt
              = @customer.frequency_of_exercise || '-'
        .vbox.row
          .col-xs-6
            %p.text-lt Smoke:
          .col-xs-6
            %p.text-lt
              = @customer.smoke || '-'
        .vbox.row
          .col-xs-6
            %p.text-lt Alcohol:
          .col-xs-6
            %p.text-lt
              = @customer.alcohol || '-'
        .vbox.row
          .col-xs-6
            %p.text-lt Diet:
          .col-xs-6
            %p.text-lt
              = @customer.diet || '-'

    %div#edit_customer_add_basic_details_form.edit-customer-basic-details-form.hide.row.panel.tab-pane.active.m.m-n
      .col-md-10
        = form_for @customer, remote: true, html: {id: 'customer_add_basic_details_form'}  do |f|
          .form-group
            = f.label 'Daily Activity'
            = f.select :daily_activity, options_for_select(['Sedentary', 'Moderate', 'Active'], selected: @customer.daily_activity), {}, {class: 'form-control'}

          .form-group
            = f.label 'Frequency of Exercise'
            = f.select :frequency_of_exercise, options_for_select(['2-3 times/wk', '3-4 times/wk', 'Daily', 'Never', 'Occasional'], selected: @customer.frequency_of_exercise), {}, {class: 'form-control'}

          .form-group
            = f.label :smoke
            = f.select :smoke, options_for_select(['None', 'Occassional', 'Often'], selected: @customer.smoke), {}, {class: 'form-control'}

          .form-group
            = f.label :alcohol
            = f.select :alcohol, options_for_select(['None', 'Occassional', 'Often'], selected: @customer.alcohol), {}, {class: 'form-control'}

          .form-group
            = f.label :diet
            = f.select :diet, options_for_select(['Vegetarian', 'Non Vegetarian', 'Vegan'], selected: @customer.diet), {include_blank: 'Select'}, {class: 'form-control'}

          .form-group
            = f.submit 'save', {class:'btn btn-sm small-btn btn-primary'}
            %a{class: 'btn btn-sm small-btn btn-danger customer-details-add-cancel-btn'} cancel

  #med_details.hide.row.panel.tab-pane.active.m.p-t.m-n
    %div#normal_customer_med_basic_details
      .col-md-10.p-b.m-b
