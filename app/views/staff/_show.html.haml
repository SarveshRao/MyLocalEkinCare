                City:
            .col-xs-6
              %p.text-lt
                = address.city || '-'
          .vbox.row
            .col-xs-6
              %p.text-lt
                State:
            .col-xs-6
              %p.text-lt
                = address.state || '-'
          .vbox.row
            .col-xs-6
              %p.text-lt
                Country:
            .col-xs-6
              %p.text-lt
                = address.country || '-'
          .vbox.row
            .col-xs-6
              %p.text-lt
                Zip code:
            .col-xs-6
              %p.text-lt
                = address.zip_code || '-'

